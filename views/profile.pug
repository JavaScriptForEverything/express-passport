doctype html
html(lang="en")
	head
		meta(charset="UTF-8")
		meta(name="viewport", content="width=device-width, initial-scale=1.0")
		title Profile | #{user.username}
	body 

		h1 User Profile: #{user.username}

		pre= JSON.stringify(user, null, 2)

		div
			a(href="/") 
				button(type="button") Home

		div
			a(href="/logout") 
				button(type="button") Logout